{
  "headers": [
    {
      "source": "/cron",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
    "crons": [
      {
        "path": "/cron",
        "schedule": "0 9 * * *"
      },
      {
        "path": "/cron",
        "schedule": "0 16 * * *"
      }
    ]
  }